{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "dom",
      "es6"
    ],
    "outDir": "./dist",
    "strict": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "../ely.core/*"
      ],
      "@enums/*": [
        "./enums/*"
      ],
      "@math/*": [
        "../ely.core/math/*"
      ],
      "@protocols/*": [
        "./protocols/*"
      ]
    },
    "experimentalDecorators": true
  },
  "exclude": [
    "./node_modules"
  ]
}
